    "maxUzawaIterations": "5000",
    "minimumViscosity": "1e-6",
    "maxStrainRate": "1e6",
    "journal.info": "True",
    "journal.debug": "True",
    "journal-level.info": "1",
    "journal-level.debug": "1"
