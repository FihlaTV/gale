<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">

<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">
<struct name="components" mergeType="merge">
	<struct name="SPR_Variable">
	      <param name="Type">Variable</param>
	      <param name="Rank">Vector</param>
	      <param name="Dependency">decomp-linear</param>
	      <param name="DataType">Double</param>
	      <param name="Count">decomp-linear-nodeDomainCount</param>
	      <param name="VectorComponentCount">6</param>
	      <list name="names">
		<param>recoveredStrainRateXX</param>
		<param>recoveredStrainRateYY</param>
		<param>recoveredStrainRateZZ</param>
		<param>recoveredStrainRateXY</param>
		<param>recoveredStrainRateXZ</param>
		<param>recoveredStrainRateYZ</param>
	      </list>

	</struct>
	<struct name="SPR_DofLayout">
	      <param name="Type">DofLayout</param>
	      <param name="Count">decomp-linear-nodeDomainCount</param>
	      <param name="BaseVariableCount">6</param>
	       <list name="BaseVariables">
	        <param>recoveredStrainRateXX</param>
		<param>recoveredStrainRateYY</param>
		<param>recoveredStrainRateZZ</param>
		<param>recoveredStrainRateXY</param>
		<param>recoveredStrainRateXZ</param>
		<param>recoveredStrainRateYZ</param>
	       </list>
	</struct>
		
	<struct name="recoveredStrainRateField">
	      <param name="Type">FeVariable</param>
	      <param name="FEMesh">mesh-linear</param>
	      <param name="DofLayout">SPR_DofLayout</param>
	</struct>
	    
	<struct name="recoveredStrainRateInvariantField">
	      <param name="Type">OperatorFeVariable</param>
	      <param name="Operator">SymmetricTensor_Invariant</param>
	      <param name="FeVariable">recoveredStrainRateField</param>
	</struct>

	<struct name="R_StrainRateXX">
	      <param name="Type">OperatorFeVariable</param>
	      <param name="Operator">TakeFirstComponent</param>
	      <param name="FeVariable">recoveredStrainRateField</param>
	</struct>

	<struct name="R_StrainRateYY">
	      <param name="Type">OperatorFeVariable</param>
	      <param name="Operator">TakeSecondComponent</param>
	      <param name="FeVariable">recoveredStrainRateField</param>
	</struct>

	<struct name="R_StrainRateZZ">
	      <param name="Type">OperatorFeVariable</param>
	      <param name="Operator">TakeThirdComponent</param>
	      <param name="FeVariable">recoveredStrainRateField</param>
	</struct>
</struct>
</StGermainData>
