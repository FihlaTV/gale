	<struct name="recoveredVGradInvariantField"> 
		<param name="Type">OperatorFeVariable</param>
		<param name="Operator">TensorInvariant</param>
		<param name="FeVariable">recoveredVGrad</param>
	</struct>
</struct>
</StGermainData>
