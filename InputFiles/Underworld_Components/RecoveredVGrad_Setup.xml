	      <param name="Type">Variable</param>
	      <param name="Rank">Vector</param>
	      <param name="Dependency">decomp-linear</param>
	      <param name="DataType">Double</param>
	      <param name="Count">decomp-linear-nodeDomainCount</param>
	      <param name="VectorComponentCount">4</param>
	      <list name="names">
		<param>recoveredVx_dx</param>
		<param>recoveredVx_dy</param>
		<param>recoveredVy_dx</param>
		<param>recoveredVy_dy</param>
	      </list>
	</struct>

	<struct name="recVGradDofLayout">
	      <param name="Type">DofLayout</param>
	      <param name="Count">decomp-linear-nodeDomainCount</param>
