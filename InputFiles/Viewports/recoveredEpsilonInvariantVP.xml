<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">

<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

<struct name="components" mergeType="merge">

	<struct name="recEpsilonInvColourMap">
		<param name="Type">lucColourMap</param>
		<param name="colours">Purple Blue Green Yellow Orange Red</param>
		<param name="dynamicRange">true</param>
	</struct>
                <struct name="EpsilonTitle" mergeType="replace">
                        <param name="Type">lucTitle</param>
                        <param name="FieldVariable">recoveredEpsilonInvariantField</param>
                </struct>

	<struct name="recoveredEpsilonInvMap">
		<param name="Type">lucScalarField</param>
		<param name="FieldVariable">recoveredEpsilonInvariantField</param>
		<param name="ColourMap">recEpsilonInvColourMap</param>
	</struct>

	<struct name="recEpsilonInvColourBar">
		<param name="Type">lucColourBar</param>
		<param name="ColourMap">recEpsilonInvColourMap</param>
	</struct>

	<struct name="recoveredEpsilonInvariantVP">
		<param name="Type">lucViewport</param>
		<param name="Camera">camera</param>
		<list name="DrawingObject">
			<param>EpsilonTitle</param>
			<param>recoveredEpsilonInvMap</param>
			<param>border</param>
			<param>recEpsilonInvColourBar</param>
		</list>
	</struct>
</struct>

</StGermainData>
