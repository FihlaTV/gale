<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">

<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

<struct name="components" mergeType="merge">

	<struct name="recoveredVGradInvColourMap">
		<param name="Type">lucColourMap</param>
		<param name="colours">Purple Blue Green Yellow Orange Red</param>
		<param name="dynamicRange">true</param>
	</struct>

	<struct name="recoveredVGradInvTitle" mergeType="replace">
		<param name="Type">lucTitle</param>
		<param name="FieldVariable">recoveredVGradInvariantField</param>
	</struct>

	<struct name="recoveredVGradInvMap">
		<param name="Type">lucScalarField</param>
		<param name="FieldVariable">recoveredVGradInvariantField</param>
		<param name="ColourMap">recoveredVGradInvColourMap</param>
	</struct>

	<struct name="recoveredVGradInvColourBar">
		<param name="Type">lucColourBar</param>
		<param name="ColourMap">recoveredVGradInvColourMap</param>
	</struct>

	<struct name="RecoveredVGradVP">
		<param name="Type">lucViewport</param>
		<param name="Camera">camera</param>
		<list name="DrawingObject">
			<param>recoveredVGradInvTitle</param>
			<param>recoveredVGradInvMap</param>
			<param>border</param>
			<param>recoveredVGradInvColourBar</param>
		</list>
	</struct>

</struct>

</StGermainData>
