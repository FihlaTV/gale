<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">

<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

<struct name="components" mergeType="merge">

	<struct name="recstressInvColourMap">
		<param name="Type">lucColourMap</param>
		<param name="colours">#111111 #220044 DarkSlateBlue Blue green yellow orange DarkRed red</param>
		<param name="dynamicRange">true</param>
	</struct>
	<struct name="recoveredStressInvMap">
		<param name="Type">lucScalarField</param>
		<param name="FieldVariable">recoveredStressInvariantField</param>
		<param name="ColourMap">recstressInvColourMap</param>
	</struct>
	<struct name="recoveredStressInvTitle" mergeType="replace">
		<param name="Type">lucTitle</param>
		<param name="string">Recovered Stress Invariant (via REP)</param>
	</struct>

	<struct name="recstressInvColourBar">
		<param name="Type">lucColourBar</param>
		<param name="ColourMap">recstressInvColourMap</param>
	</struct>

	<struct name="recoveredStressInvariantVP">
		<param name="Type">lucViewport</param>
		<param name="Camera">camera</param>
		<list name="DrawingObject">
			<param>recoveredStressInvMap</param>
			<param>border</param>
			<param>recoveredStressInvTitle</param>
			<param>recstressInvColourBar</param>
		</list>
	</struct>
</struct>

</StGermainData>
