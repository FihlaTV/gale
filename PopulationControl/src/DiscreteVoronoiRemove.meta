<param name="Summary"></param>
<param name="Description">This component constructs a Voronoi diagram for each of the points in a cell and removes particles with the smallest area/volume so that the final number of points in a cell is 'idealParticleCount'.</param>

<!--Now the interesting stuff-->
<list name="Params">
</list>

<list name="Dependencies">
	<struct>
		<param name="Essential">True</param>
		<param name="Name">DiscreteVoronoi</param>
		<param name="Type">DiscreteVoronoi</param>
		<param name="Description">The component used to construct the Discrete Voronoi diagram.</param>
	</struct>
</list>
<!-- Add an exmaple XML if possible -->
<param name="Example">
<![CDATA[
		<struct name="removalRoutine">
			<param name="Type">DiscreteVoronoiRemove</param> 
			<param name="idealParticleCount">20</param>
			<param name="DiscreteVoronoi">discreteVoronoi</param>
		</struct> 
]]>

</param>

