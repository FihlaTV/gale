<param name="Name">SplittingRoutine</param>
<param name="Author">Robert Turnbull</param>
<param name="Organisation">VPAC and MCC</param>
<param name="Project">PICellerator</param>
<param name="Location">./PICellerator/PopulationControl/src/</param>
<param name="Project Web">http://www.stgermainproject.org/PICellerator.html</param>
<param name="Copyright">Copyright (C) 2005 VPAC and Monash Cluster Computing.</param>
<param name="License">https://csd.vpac.org/twiki/bin/view/Stgermain/SoftwareLicense http://www.opensource.org/licenses/bsd-license.php</param>
<param name="Parent">Stg_Component</param>
<param name="Reference"></param>
<param name="Summary"></param>
<param name="Description">An abstract class for components that split particles from cells if they get too sparse. Generally, the more particles in a cell, the higher the accuracy of the calculation, however if there are too many particles leads to prohibitive memory use and calculation time.</param>

<!--Now the interesting stuff-->


<list name="Params">
	<struct>
		<param name="Name">idealParticleCount</param>
		<param name="Type">UnsignedInt</param>
		<param name="Default">0</param>
		<param name="Description">The ideal number of particles in a cell which you wish to keep the number of particles in a cell to. If a cell has less particles than the 'minParticlesPerCell' value, then the routine will increase the number of particles to 'idealParticleCount'.</param>
	</struct>
	<struct>
		<param name="Name">minParticlesPerCell</param>
		<param name="Type">UnsignedInt</param>
		<param name="Default">idealParticleCount</param>
		<param name="Description">The minimum number of particles in a cell before this component begins to split particles. By default it is set to the parameter 'idealParticleCount'.</param>
	</struct>

</list>

<list name="Dependencies">
</list>

<!-- Add an exmaple XML if possible -->
<param name="Example">N/A</param>

</StGermainData>
